<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.expedia.evc.persistence.dao.IMerchantTypeDao">

	<select id="getMerchantTypes" resultType="MerchantTypeDO">
		SELECT mt.MerchantTypeID as 'typeId',
		       mt.MerchantType as 'typeName'
		FROM dbo.MerchantType mt
	</select>

</mapper>
